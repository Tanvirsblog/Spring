package com.xworkz.cctv;

public class CCTV {

	public String brand;
	public int cameraSize;
	public double price;
	public String color;

	public CCTV() {
		System.out.println(this.getClass().getSimpleName() + "object created");
	}

	public void record() {
		System.out.println("Invoked record");
	}

	public void autoDelete() {
		System.out.println("Invoked autoDelete");

	}

}
